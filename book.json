{
    "title": "Leap 框架官方开发文档",

    "plugins": [
        "-sharing",
        "-fontsettings",
        "-highlight",
        "prism",
        "terminal",
        "include-codeblock",
        "cuav-chapters",
        "ace",
        "anchor-navigation-ex"
    ],

    "pluginsConfig": {
        "terminal": { 
            "copyButtons": false,
            "fade": false
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": false,
            "mode": "float",
            "float": {
                "showLevelIcon": false
            }
        }
    },

    "styles": {
        "website": "styles/website.css"
    },
    
    "variables": {
        "leap": {
            "version": "0.6.2b"
        }
    } 
    
}